{
  "name": "aws-cdk-circleci-sample",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-circleci-sample": "bin/aws-cdk-circleci-sample.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "ci_diff": "cdk diff 2>&1 | sed -r 's/\\x1B\\[([0-9]{1,2}(;[0-9]{1,2})?)?[mGK]//g' || true",
    "ci_deploy": "cdk deploy"
  },
  "devDependencies": {
    "@types/node": "8.10.45",
    "typescript": "^3.3.3333",
    "aws-cdk": "^0.31.0"
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "^0.31.0",
    "@aws-cdk/cdk": "^0.31.0",
    "source-map-support": "^0.5.9"
  }
}